{
  "name": "headshot-studio-pro",
  "version": "1.0.0",
  "description": "AI-powered professional headshot generation and Instagram automation platform",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "db:push": "cd packages/database && pnpm db:push",
    "db:studio": "cd packages/database && pnpm db:studio",
    "db:generate": "cd packages/database && pnpm db:generate"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.6.0",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@10.4.1"
}
